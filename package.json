    "@mdx-js/tag": "^0.14.1",
    "chalk": "^2.4.1",
    "clipboardy": "^1.2.3",
    "gray-matter": "^4.0.1",
    "lodash.debounce": "^4.0.8",
    "meow": "^5.0.0",
    "ok-cli": "^3.0.4",
    "pkg-conf": "^2.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dev-utils": "^5.0.1",
    "stringify-object": "^3.2.2",
    "styled-components": ">=3.0.0",
    "styled-system": "^3.0.2",
    "superbox": "^2.1.0",
    "webpack-hot-client": "^4.1.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "jest": "^23.4.2",
    "jest-styled-components": "^5.0.1",
    "prettier": "^1.14.0",
    "react-test-renderer": "^16.4.1"
  },
  "jest": {
    "roots": [
